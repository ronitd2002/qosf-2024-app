def qft(circuit,n):
    n = circuit.num_qubits
    for i in range(n):
        circuit.h(i)
        for j in range(i + 1, n):
            circuit.cp(np.pi / 2**(j - i), j, i)
        #circuit.barrier()
    #for i in range(n // 2):
        #circuit.swap(i, n - i - 1)
    return circuit

def iqft(circuit,n):
    n = circuit.num_qubits
    circuit = qft(circuit,n)
    return circuit.inverse()





